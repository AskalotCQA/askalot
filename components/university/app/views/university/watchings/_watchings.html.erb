<%= form_messages key: tab %>

<% if watchings.any? %>
  <ol class="watchings">
    <% watchings.each do |watching| %>
      <%= render 'university/watching', watching: watching %>
    <% end %>
  </ol>

  <%= render 'university/clean', model: tab.to_s.singularize %>

  <%= paginate watchings, params: { tab: tab } %>
<% else %>
  <div class="alert alert-info">
    <%= t 'watching.none' %>
  </div>
<% end %>
