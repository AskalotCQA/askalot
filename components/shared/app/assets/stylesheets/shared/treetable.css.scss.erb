.treetable {
    .treetable-expander {
        width: 16px;
        height: 16px;
        display: inline-block;
        position: relative;
        cursor: pointer;
    }

    .treetable-expander-expanded {
        background-image: url(<%= asset_path 'shared/treetable/collapse.png' %>);
    }

    .treetable-expander-collapsed {
        background-image: url(<%= asset_path 'shared/treetable/expand.png' %>);
    }

    .treetable-indent {
        width: 16px;
        height: 16px;
        display: inline-block;
        position: relative;
    }
}