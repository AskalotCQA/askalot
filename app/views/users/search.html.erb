<% provide(:title, t('user.search.title')) %>

<%= render 'users/searching', target: controller_name %>

<ul id="users-tabs" class="nav nav-tabs tab-navigation">
  <%= tab_link_tag_with_count t('user.tab.all'), :results, tags_path(params.slice(:q, :page)), @results.total_entries, analytics_attributes(:users, :search, :results).merge(remote: true, data: { fade: '#users' }) %>
</ul>

<%= render 'users', users: @results, remote: false %>
