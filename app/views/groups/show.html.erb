<% provide :title, @group.title %>

<div class="row">
  <div class="col-md-offset-3 col-md-9">
    <h1><%= @group.title %></h1>
  </div>
</div>
<div class="row">
  <div class="col-md-3">
        Menu
  </div>
  <div class="col-md-9">
    <ul>
      <% @documents.each do |document| %>
        <li>
          <%= render 'documents/document' %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
